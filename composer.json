{
    "name": "digitalcloud/nova-resource-notes",
    "description": "A Laravel Nova Resource Note.",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/tott/laravel-model-notes"
        }
    ],
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Motaz Marouf",
            "email": "m.marouf@dce.sa",
            "homepage": "https://dce.sa",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0 || >=8.0",
        "tott/laravel-model-notes": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "DigitalCloud\\NovaResourceNotes\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DigitalCloud\\NovaResourceNotes\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
